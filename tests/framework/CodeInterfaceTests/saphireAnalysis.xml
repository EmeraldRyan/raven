<?xml version="1.0" ?>
<Simulation verbosity="debug">
  <TestInfo>
    <name>saphire</name>
    <author>wangc</author>
    <created>2018-08-31</created>
    <classesTested>CodeInterface</classesTested>
    <description>
       This test is for Sept Milestone
    </description>
  </TestInfo>
  <RunInfo>
    <JobName>saphire</JobName>
    <Sequence>sample</Sequence>
    <WorkingDir>saphireAnalysis</WorkingDir>
    <batchSize>1</batchSize>
  </RunInfo>

  <Files>
    <Input name="macro" type="">Class_Chg_DEMO.mac</Input>
    <Input name="demo" type="">demo.zip</Input>
  </Files>

  <Models>
    <Code name="saphire" subType="Saphire">
      <executable>"C:\Saphire 8\tools\SAPHIRE.exe"</executable>
      <clargs arg="macro" extension=".mac" type="input" delimiter="="/>
      <clargs arg="project" extension=".zip" type="input" delimiter="="/>
      <outputFile>fixed_output.csv</outputFile>
      <codeOutput type="uncertainty">et_uq.csv</codeOutput>
      <codeOutput type="uncertainty">ft_uq.csv</codeOutput>
    </Code>
  </Models>

  <Distributions>
    <Uniform name="CKV-CC-EVENTS">
	<lowerBound>1.0E-5</lowerBound>
	<upperBound>1.0E-3</upperBound>
    </Uniform>
    <Uniform name="MOV-1-EVENTS">
	<lowerBound>2.0E-5</lowerBound>
	<upperBound>2.0E-3</upperBound>
    </Uniform>
    <Uniform name="MOV-CC-EVENTS">
	<lowerBound>1.0E-4</lowerBound>
	<upperBound>1.0E-2</upperBound>
    </Uniform>
    <Uniform name="PMP-FS-EVENTS">
	<lowerBound>1.2E-4</lowerBound>
	<upperBound>1.2E-2</upperBound>
    </Uniform>
    <Uniform name="DGN-FS-EVENTS">
	<lowerBound>4.0E-4</lowerBound>
	<upperBound>4.0E-2</upperBound>
    </Uniform>
    <Uniform name="CKV-1-EVENTS">
	<lowerBound>9.74E-2</lowerBound>
	<upperBound>1.0</upperBound>
    </Uniform>
    <Uniform name="CKV-2-EVENTS">
	<lowerBound>2.05E-3</lowerBound>
	<upperBound>2.05E-1</upperBound>
    </Uniform>
    <Uniform name="PMP-FR-EVENTS-lambda">
	<lowerBound>2.1E-6</lowerBound>
	<upperBound>2.1E-4</upperBound>
    </Uniform>
    <Uniform name="PMP-FR-EVENTS-Tm">
	<lowerBound>1.2E1</lowerBound>
	<upperBound>3.6E1</upperBound>
    </Uniform>
    <Uniform name="DGN-FR-EVENTS-lambda">
	<lowerBound>8.9E-5</lowerBound>
	<upperBound>8.9E-3</upperBound>
    </Uniform>
    <Uniform name="DGN-FR-EVENTS-Tm">
	<lowerBound>1.2E1</lowerBound>
	<upperBound>3.6E1</upperBound>
    </Uniform>
